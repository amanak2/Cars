<div class="form-inline clearfix">
  <div class="nested-fields">
      <%= f.input :engine_type, label: "Type", input_html: {class: "form-input form-control"} %>
      <%= f.input :displacment, input_html: {class: "form-input form-control"} %>
      <%= f.input :power, input_html: {class: "form-input form-control"} %>
      <%= f.input :torque, input_html: {class: "form-input form-control"} %>
      <%= f.input :layout, input_html: {class: "form-input form-control"} %>
